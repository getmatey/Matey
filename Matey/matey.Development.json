{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Docker": {
    "Endpoint": "npipe://./pipe/docker_engine"
  },
  "PKI": {
    "ACME": {
      "AcceptTermsOfService": true,
      "AccountContactEmails": [ "joe.bloggs@example.com" ],
      "Environment": "Development",
      "Environments": [
        {
          "Name": "Development",
          "CertificateAuthorityUri": "https://localhost/acme/acme/",
          "UsePostAsGet": true
        }
      ],
      "Http01": {
        "Bindings": [
          {
            "HostName": "*",
            "Port": 8080
          }
        ]
      }
    },
    "CertificateStore": {
      "StoreLocation": "LocalMachine",
      "StoreName": "Root"
    }
  }
}
